<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<featureModel>
	<properties>
		<graphics key="legendautolayout" value="true"/>
		<graphics key="showshortnames" value="false"/>
		<graphics key="layout" value="horizontal"/>
		<graphics key="showcollapsedconstraints" value="true"/>
		<graphics key="legendhidden" value="false"/>
		<graphics key="layoutalgorithm" value="1"/>
	</properties>
	<struct>
		<and abstract="true" mandatory="true" name="GestoreParcheggio">
			<graphics key="collapsed" value="false"/>
			<feature mandatory="true" name="Ticket"/>
			<alt abstract="true" mandatory="true" name="pagamento">
				<graphics key="collapsed" value="false"/>
				<feature name="Erogazione"/>
				<feature name="Consegna"/>
			</alt>
			<or abstract="true" mandatory="true" name="MetodoPagamento">
				<graphics key="collapsed" value="false"/>
				<feature name="CC"/>
				<feature name="contanti"/>
				<feature name="moneta"/>
				<feature name="abbonamento"/>
			</or>
			<alt abstract="true" mandatory="true" name="Guardia">
				<feature name="Barrier"/>
				<feature name="Barrierless"/>
			</alt>
			<and mandatory="true" name="BaseDati">
				<graphics key="collapsed" value="false"/>
				<feature mandatory="true" name="TicketTable"/>
				<feature name="TargheTable"/>
			</and>
			<alt mandatory="true" name="TerminaliHW">
				<feature name="unico"/>
				<feature name="separati"/>
			</alt>
		</and>
	</struct>
	<constraints>
		<rule>
			<imp>
				<var>Barrierless</var>
				<conj>
					<var>Erogazione</var>
					<var>TargheTable</var>
				</conj>
			</imp>
		</rule>
		<rule>
			<imp>
				<var>Barrier</var>
				<conj>
					<var>Consegna</var>
					<var>separati</var>
				</conj>
			</imp>
		</rule>
	</constraints>
</featureModel>
