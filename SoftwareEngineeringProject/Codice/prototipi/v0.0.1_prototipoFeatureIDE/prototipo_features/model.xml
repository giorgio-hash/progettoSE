<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<featureModel>
	<properties>
		<graphics key="legendautolayout" value="true"/>
		<graphics key="showshortnames" value="false"/>
		<graphics key="layout" value="horizontal"/>
		<graphics key="showcollapsedconstraints" value="true"/>
		<graphics key="legendhidden" value="false"/>
		<graphics key="layoutalgorithm" value="1"/>
	</properties>
	<struct>
		<and abstract="true" mandatory="true" name="ProgettoIngegneriaDelSoftware">
			<alt mandatory="true" name="Parcheggio">
				<feature name="Sbarra"/>
				<feature name="SenzaSbarra"/>
			</alt>
			<and mandatory="true" name="Databse">
				<feature mandatory="true" name="Ticket"/>
				<feature name="Targhe"/>
			</and>
			<alt mandatory="true" name="Pagamento">
				<feature name="Telepass"/>
				<feature name="Contanti"/>
				<feature name="Carta"/>
			</alt>
		</and>
	</struct>
	<constraints>
		<rule>
			<imp>
				<var>Sbarra</var>
				<conj>
					<var>Ticket</var>
					<not>
						<var>Targhe</var>
					</not>
				</conj>
			</imp>
		</rule>
		<rule>
			<imp>
				<var>SenzaSbarra</var>
				<conj>
					<var>Ticket</var>
					<var>Targhe</var>
				</conj>
			</imp>
		</rule>
	</constraints>
</featureModel>
